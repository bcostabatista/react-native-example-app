{
  "expo": {
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0",
    "name": "Example app",
    "description": "Example app",
    "privacy": "unlisted",
    "slug": "example-app",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 7000,
      "enabled": true,
      "checkAutomatically": "ON_LOAD"
    },
    "notification" : {
      "icon": "./assets/images/icon.png"
    },
    "android": {
      "package": "example.app",
      "versionCode": 1
    },
    "ios": {
      "bundleIdentifier": "example-app",
      "buildNumber": "1",
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["example-app"]
      },
      "supportsTablet": true,
      "icon": "./assets/images/icon.png"
    }
  }
}
